extends layout
block content
  p Name: <b>#{user.username}</b>
  p EmailAdress: <b>#{user.email}</b>

  p comments:
  table
    tr
      th title
      th creationDate
      th content
      each val in comments
      tr 
        td #{val.title}
        td #{val.publish_date}
        td #{val.text}
      
